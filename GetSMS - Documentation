								<th scope="col">Значение</th>
							</tr>
							</thead>
							<tbody>
							<tr>
								<td><code>ORDER_NOT_FOUND</code></td>
								<td>Заказ не найден</td>
							</tr>
							</tbody>
						</table>
						<p></p>
					</section>

					<section class="docs-section" id="orders-rac">
						<h2 class="section-heading">Получить повторное смс</h2>
						<p>
						</p><h5>Метод</h5>
						<code>POST /orders/request_another_code</code>
						<p></p>

						<p>
